{
  "terminal": {
    "fullscreen": true,
    "width": 120,
    "height": 102,
    "display_mode": 2,
    "fps_limit_enabled": true,
    "fps_limit": 60
  },

  "camera": {
    "view_point": [ 1.5, -0.5, 0.5 ],
    "view_angle_x": 0.01,
    "view_angle_z": 3.1415,
    "fov": 35
  }, 

  "octree": {
    "center": [ 0.5, 0.5, 0.5 ],
    "side_length": 1,
    "depth": 4,
    "same_voxel_size": true
  },

  "light": {
    "position": [ 20, -10, 30 ]
  },

  "renderer": {
    "max_ray_bounces": 10,
    "grayscale": " .:-=+*#%@",
    "shadows_enabled": false,
    "skybox_enabled": true,
    "skybox": "assets/checkermap2.jpg"
  }
}
